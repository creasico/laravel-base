{
    "name": "creasi/laravel-base",
    "description": "Accounts Management Package",
    "keywords": ["laravel", "accounts", "package"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Creasi Developers",
            "email": "developers@creasi.co"
        }
    ],
    "support": {
        "source": "https://github.com/creasico/laravel-base"
    },
    "scripts": {
        "fix": [
            "pint --preset laravel"
        ],
        "test": [
            "testbench package:test --coverage"
        ],
        "testbench": [
            "testbench"
        ]
    },
    "autoload": {
        "psr-4": {
            "Creasi\\Base\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creasi\\Tests\\": "tests",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^8.1",
        "creasi/laravel-nusa": "^0.1.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "composer-runtime-api": "*",
        "laravel/dusk": "^7.0",
        "laravel/pint": "^1.1",
        "nunomaduro/collision": "^7.4",
        "orchestra/testbench": "^8.5"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creasi\\Base\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
